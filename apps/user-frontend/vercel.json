{
  "version": 2,
  "name": "vinhxuan-public",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd ../.. && yarn install && yarn workspace @vinhxuan/shared build && cd apps/user-frontend && yarn build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "cd ../.. && yarn install"
}
